{
  "manifest_version": 3,
  "name": "个性化主页",
  "version": "1.0.0",
  "description": "一个美观的浏览器主页",
  "icons": {
    "16": "./icons/16.png",
    "32": "./icons/32.png",
    "48": "./icons/48.png",
    "128": "./icons/128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "action": {
    "default_popup": "index.html",
    "default_title": "打开主页"
  },
  "permissions": [
    "storage"
  ]
}

